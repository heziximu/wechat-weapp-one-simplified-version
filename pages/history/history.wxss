/* history.wxss */
.container {
  border-top: 0;
}

.types {
  width: 100%;
  height: 30rpx;
  position: fixed;
  display: flex;
  padding: 20rpx 0;
  border-top: 2rpx solid #ebebeb;
  background-color: #fff;
  z-index: 10;
}

.fix-fixed {
  width: 100%;
  height: 60rpx;
}

.type-item {
  flex: 1;
  font-size: 28rpx;
  text-align: center;
}

.seleted {
  color: #89bdf7;
}

.history-bymonth {
  height: 40rpx;
  padding: 20rpx;
  margin: 0 20rpx;
  border-bottom: 1px solid #ebebeb;
}

.history-date {
  color: #888;
  font-size: 28rpx;
  line-height: 40rpx;
}
